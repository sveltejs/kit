{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"kit.svelte.dev#build": {
			"dependsOn": ["^build"],
			"inputs": ["src/**", "../../packages/kit/docs/**", "../../documentation/**"],
			"outputs": [".vercel_build_output/**", ".vercel/output/**"],
			"outputMode": "new-only",
			"env": ["VERCEL", "ENABLE_VC_BUILD"]
		},
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["src/**", "scripts/**", "shared/**", "templates/**"],
			"outputs": [
				"files/**",
				"dist/**",
				".svelte-kit/**",
				".vercel_build_output/**",
				".vercel/output/**"
			],
			"outputMode": "new-only",
			"env": ["VERCEL", "ENABLE_VC_BUILD"]
		},
		"check": {
			"inputs": [
				"index.js",
				"index.d.ts",
				"ambient.d.ts",
				"src/**",
				"source/**",
				"types/**",
				".svelte-kit/types/**",
				".custom-out-dir/types/**",
				".svelte-kit/tsconfig.json"
			],
			"outputs": [],
			"outputMode": "new-only"
		},
		"format": {
			"cache": false,
			"outputs": [],
			"outputMode": "new-only"
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": ["coverage/", "test-results/**"],
			"outputMode": "new-only",
			"env": ["CI", "TURBO_CACHE_KEY"]
		},
		"test:cross-browser": {
			"outputs": ["coverage/", "test-results/**"],
			"outputMode": "new-only",
			"env": ["CI", "TURBO_CACHE_KEY"]
		},
		"lint": {
			"outputs": [],
			"outputMode": "new-only"
		},
		"precommit": {
			"dependsOn": ["format", "lint"],
			"outputs": [],
			"outputMode": "new-only"
		},
		"dev": {
			"cache": false,
			"outputMode": "new-only"
		}
	},
	"globalDependencies": [".eslintrc.json", ".prettierrc", "tsconfig.json"]
}
