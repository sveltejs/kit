{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"strict": true,
		"target": "es2022",
		"module": "es2022",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"paths": {
			"@sveltejs/kit": ["../../../../exports/public"],
			"types": ["../../../../types/internal"]
		}
	},
	"include": ["./**/*.js", "./**/*.ts"],
	"exclude": ["./**/.svelte-kit/**"]
}
