{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"strict": true,
		"target": "es2020",
		"module": "es2022",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"paths": {
			"types": ["./types/index"]
		},
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"include": ["src/**/*", "types/**/*"]
}
